# 在 Python 中使用"""建立多行字串
multi_line_string = """this is line number 1
this is line number 2
this is line number 3
this is line number 4"""

print(multi_line_string)

# 在 Python 中使用 () 建立多行字串
multi_line_string = ("this is line number 1 "
"this is line number 2 "
"this is line number 3 "
"this is line number 4")

print(multi_line_string)

# 在 Python 中使用\建立一個多行字串
multi_line_string = "this is line number 1 " \
"this is line number 2 " \
"this is line number 3 " \
"this is line number 4" \

print(multi_line_string)
